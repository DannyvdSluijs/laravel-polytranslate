{
  "name": "deployteam/laravel-polytranslate",
  "description": "Add multiple locations for Laravel localization namespace",
  "homepage": "http://deploy.com.ro",
  "keywords": ["laravel"],
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Hampu",
      "email": "alex.hampu@deploy.com.ro"
    }
  ],
  "autoload" : {
    "psr-4" : {
      "DeployTeam\\PolyTranslate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DeployTeam\\PolyTranslate\\Test\\": "tests"
    }
  },
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "laravel/framework": "~7.0",
    "orchestra/testbench": "^5.1"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "DeployTeam\\PolyTranslate\\ServiceProvider"
      ],
      "aliases": {
        "PolyTranslate": "DeployTeam\\PolyTranslate\\Facade"
      }
    }
  }
}
